<%- include("partials/head.ejs") %> <%- include("partials/header.ejs", {logout:
'false'}) %>
<div class="flex flex-col justify-center items-center">
  <form
    method="POST"
    class="bg-white p-6 rounded-lg shadow-md w-full md:w-1/2 lg:w-1/3 xl:w-1/4 flex flex-col items-center"
  >
    <div class="flex flex-col mb-4 w-full">
      <label name="email" class="text-base mb-2">Enter your email:</label>
      <input name="email" class="border border-violet-900 rounded-md p-2" />
    </div>
    <div class="flex flex-col mb-4 w-full">
      <label name="password" class="text-base mb-2">Enter your password:</label>
      <input
        type="password"
        name="password"
        class="border border-violet-900 rounded-md p-2"
      />
    </div>
    <div class="flex gap-16 items-center">
      <a href="/" class="bg-red-200 text-white py-2 px-6 rounded-md">Cancel</a>
      <button class="bg-violet-900 text-white py-2 px-6 rounded-md">
        Logon
      </button>
    </div>
  </form>
</div>
<%- include("partials/footer.ejs") %>
